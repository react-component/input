{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "lib": ["dom", "es2017", "es2022", "esnext"],
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "declaration": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": [".dumi/tmp/*"],
      "@rc-component/input": ["src/index.tsx"]
    }
  },
  "include": [".dumirc.ts", "**/*.ts", "**/*.tsx"]
}
